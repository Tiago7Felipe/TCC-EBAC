pipeline {
    agent any

    stages {
        stage('Clonar o Reposit√≥rio') {
            steps {
                git branch: 'main', url: 'https://github.com/Tiago7Felipe/NATIVE-DEMO-APP.git'
            }
        }
                stage('instalar dependencias') {
            steps {
                powershell 'npm ci'
            }
        }
                stage('executar os testes') {
            steps {
                powershell 'npm run cy:run'
            }
        }
    }
}